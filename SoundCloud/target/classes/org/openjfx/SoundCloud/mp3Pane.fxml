<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="91.0" prefWidth="1081.0" style="-fx-background-color: BLACK;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.SoundCloud.MP3Controller">
   <children>
      <ProgressBar fx:id="songProgressBar" layoutX="367.0" layoutY="71.0" prefHeight="8.0" prefWidth="350.0" progress="0.0" style="-fx-background-radius: 20px; -fx-border-radius: 20px; -fx-background-color: BLACK; -fx-border-color: BLACK;" />
      <Button fx:id="playButton" alignment="CENTER" layoutX="521.0" layoutY="12.0" mnemonicParsing="false" onAction="#playButtonClicked" prefHeight="45.0" prefWidth="45.0" style="-fx-background-radius: 100px; -fx-border-radius: 100px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fx:id="playButtonImageView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@playButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="previousButton" alignment="CENTER" contentDisplay="CENTER" layoutX="461.0" layoutY="15.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="40.0" prefWidth="40.0" style="-fx-background-radius: 100px; -fx-border-radius: 100px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@previousButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Slider fx:id="volumeSlider" layoutX="940.0" layoutY="40.0" max="100.0" prefHeight="8.0" prefWidth="127.0" style="-fx-background-color: BLACK; -fx-border-color: BLACK;" stylesheets="@styles.css" value="50.0" />
      <Button fx:id="nextButton" layoutX="581.0" layoutY="15.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="40.0" prefWidth="40.0" style="-fx-background-radius: 50px; -fx-border-radius: 50px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@nextButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="songLabel" layoutX="95.0" layoutY="22.0" prefHeight="17.0" prefWidth="151.0" text="Label" textFill="WHITE" />
      <Button fx:id="lyricButton" layoutX="853.0" layoutY="33.0" mnemonicParsing="false" onAction="#showSongLyric" style="-fx-background-color: #232323; -fx-background-radius: 20px; -fx-border-radius: 20px; -fx-border-color: #ffffff;" text="Show lyric" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <ImageView fx:id="songImageView" fitHeight="75.0" fitWidth="75.0" layoutX="9.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
