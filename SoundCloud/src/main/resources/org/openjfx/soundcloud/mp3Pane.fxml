<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="92.0" prefWidth="930.0" style="-fx-background-color: BLACK;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.SoundCloud.MP3Controller">
   <children>
      <ProgressBar fx:id="songProgressBar" layoutX="290.0" layoutY="62.0" prefHeight="8.0" prefWidth="350.0" progress="0.0" style="-fx-background-radius: 20px; -fx-border-radius: 20px; -fx-background-color: BLACK; -fx-border-color: BLACK;" />
      <Button fx:id="playButton" alignment="CENTER" layoutX="447.0" layoutY="11.0" mnemonicParsing="false" onAction="#playButtonClicked" prefHeight="40.0" prefWidth="40.0" style="-fx-background-radius: 50px; -fx-border-radius: 50px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fx:id="playButtonImageView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@playButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="previousButton" layoutX="373.0" layoutY="16.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="30.0" prefWidth="30.0" style="-fx-background-radius: 50px; -fx-border-radius: 50px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@previousButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Slider fx:id="volumeSlider" layoutX="798.0" layoutY="61.0" max="100.0" prefHeight="8.0" prefWidth="127.0" style="-fx-background-color: BLACK; -fx-border-color: BLACK;" stylesheets="@styles.css" value="50.0" />
      <Button fx:id="nextButton" layoutX="524.0" layoutY="16.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="30.0" prefWidth="30.0" style="-fx-background-radius: 50px; -fx-border-radius: 50px; -fx-background-color: WHITE;">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@nextButton.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="8.0" layoutY="12.0" prefHeight="71.0" prefWidth="73.0" style="-fx-background-color: WHITE;" />
      <Label fx:id="songLabel" layoutX="95.0" layoutY="22.0" prefHeight="17.0" prefWidth="255.0" text="Label" textFill="WHITE" />
      <Button fx:id="lyricButton" layoutX="696.0" layoutY="27.0" mnemonicParsing="false" onAction="#showSongLyric" text="Button" />
   </children>
</AnchorPane>
